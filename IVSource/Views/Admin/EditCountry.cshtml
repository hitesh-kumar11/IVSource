
@{
    Layout = "~/Views/Shared/Admin/_LayoutAdmin.cshtml";
}


<br />
<div class="container row p-0 m-0">
    <div class="col-6">
        <h2 class="text-info">Country List</h2>
    </div>
    <div class="col-3 offset-3">
        <a asp-action="" asp-controller="" class="btn btn-info form-control text-white">
            Add New Country
        </a>
    </div>
    <div class="col-12 border p-3">
        <table id="DT_load" class="table table-striped table-bordered" style="width:100%">
            <thead>
                <tr>
                    <th>SERIAL NO.</th>
                    <th>COUNTRY NAME</th>
                    <th>COUNTRY ISO</th>
                    <th>IS ENABLED</th>
                    <th>ACTION</th>
                </tr>
            </thead>
        </table>
    </div>
</div>

@section Scripts{ 
    <script src="~/js/CountryList.js"></script>
}

